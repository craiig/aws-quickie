export AWS_ACCESS_KEY_ID=
export AWS_SECRET_ACCESS_KEY=
export AWS_DEFAULT_REGION=us-west-2

#you can usually leave this as is unless you want to use a different
#identity file than the autogenerated one
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
export AWS_IDENTITY_FILE=$DIR/identity.pem
